<Window x:Class="client.login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="login" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="64,192,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" IsDefault="True" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="145,192,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" IsCancel="True" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="89,55,0,0" Name="textBox1" VerticalAlignment="Top" Width="120" Text="elias" />
        <Label Content="User Name" Height="28" HorizontalAlignment="Left" Margin="12,53,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Password" Height="28" HorizontalAlignment="Left" Margin="12,77,0,0" Name="label2" VerticalAlignment="Top" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="89,82,0,0" Name="passwordBox1" VerticalAlignment="Top" Width="120" Password="12345" />
    </Grid>
</Window>
